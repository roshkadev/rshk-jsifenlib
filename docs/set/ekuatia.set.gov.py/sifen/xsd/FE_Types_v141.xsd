<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
	elementFormDefault="qualified"
	xmlns="http://ekuatia.set.gov.py/sifen/xsd"
	targetNamespace="http://ekuatia.set.gov.py/sifen/xsd">

	<xs:simpleType name="tEmail">
		<xs:restriction base="xs:string">
			<xs:pattern
				value="([0-9a-zA-Z#$%]([-.\w]*[0-9a-zA-Z#$%'\.\-_])*@([0-9a-zA-Z][-\w]*[0-9a-zA-Z]\.)+[a-zA-Z]{2,9})" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="tDesPais">
		<xs:restriction base="noEmptyString">
			<xs:whiteSpace value="preserve"/>
			<xs:minLength value="4"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- CDC -->
	<xs:simpleType name="tCDC">
		<xs:annotation>
			<xs:documentation>
				Codigo de Control del Documento Electronico
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{2}([0-9]{7}[0-9A-D])[0-9]{34}"/>
			<xs:length value="44"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de Emision -->
	<xs:simpleType name="tiTipEmi">
		<xs:annotation>
			<xs:documentation>
				Tipo de Emision: 1(Normal), 2(Contingencia)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="[1-2]"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del tipo de emision -->
	<xs:simpleType name="tdDesTipEmi">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de emision: 1(Normal). 2(Contingencia)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="6"/>
			<xs:maxLength value="12"/>
			<xs:enumeration value="Normal"/>
			<xs:enumeration value="Contingencia"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo Aleatoria generado por el emisor -->
	<xs:simpleType name="tiCodSe">
		<xs:annotation>
			<xs:documentation>
				La generacion de este codigo es responsabilidad del emisor, segun los
				delineamientos
				establecidos en el
				Manual Tecnico, no debe ser un numero secuencial, sino aleatorio, tampoco
				debe
				contener solo ceros
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0-9]{9}"/>
			<xs:minInclusive value="1"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de Indicador de presencia -->
	<xs:simpleType name="tiIndPres">
		<xs:annotation>
			<xs:documentation>
				Indicador de presencia
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del Indicador de presencia -->
	<xs:simpleType name="tdDesIndPres">
		<xs:annotation>
			<xs:documentation>
				Descripcion del indicador de presencia
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Operación presencial"/>
					<xs:enumeration value="Operación electrónica"/>
					<xs:enumeration value="Operación telemarketing"/>
					<xs:enumeration value="Venta a domicilio"/>
					<xs:enumeration value="Operación bancaria"/>
					<xs:enumeration value="Operación cíclica"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="10"/>
					<xs:maxLength value="30"/>
					<xs:pattern value=".+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>

	<!-- Tipo de transaccion -->
	<xs:simpleType name="tiTipTra">
		<xs:annotation>
			<xs:documentation>
				Tipo de transaccion
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="7"/>
			<xs:enumeration value="8"/>
			<xs:enumeration value="9"/>
			<xs:enumeration value="10"/>
			<xs:enumeration value="11"/>
			<xs:enumeration value="12"/>
			<!-- <xs:enumeration value="99"/> -->
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del tipo de transaccion -->
	<xs:simpleType name="tdDesTiTran">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de transaccion
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Venta de mercadería"/>
			<xs:enumeration value="Prestación de servicios"/>
			<xs:enumeration
				value="Mixto (Venta de mercadería y servicios)"/>
			<xs:enumeration value="Venta de activo fijo" />
			<xs:enumeration value="Venta de divisas"/>
			<xs:enumeration value="Compra de divisas"/>
			<xs:enumeration
				value="Promociones o entrega de muestras"/>
			<xs:enumeration value="Donación"/>
			<xs:enumeration value="Anticipo"/>
			<xs:enumeration value="Compra de productos"/>
			<xs:enumeration value="Compra de servicios"/>
			<xs:enumeration value="Venta de crédito fiscal"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Informacion del Emisor -->
	<xs:simpleType name="tdInfoEmi">
		<xs:annotation>
			<xs:documentation>
				Informacion de interes del emisor respecto al DE
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3000"/>
			<xs:pattern value=".+"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Informaciona del Fisco -->
	<xs:simpleType name="tdInfoFisc">
		<xs:annotation>
			<xs:documentation>
				Informacion de interes del Fisco respecto al DE
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="3000"/>
			<xs:pattern value=".+"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de documento electronico -->
	<xs:simpleType name="tiTiDE">
		<xs:annotation>
			<xs:documentation>
				Tipo de documento electronico
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="1|[5-6]"/>  <!-- patrón temporal -->			
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del tipo de documento electronico -->
	<xs:simpleType name="tdDesTiDE">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de documento electronico
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
				<xs:enumeration value="Factura electrónica"/>
<!-- 			<xs:enumeration -->
<!-- 				value="Factura electrónica de exportación"/> -->
<!-- 			<xs:enumeration -->
<!-- 				value="Factura electrónica de importación"/> -->
<!-- 			<xs:enumeration value="Autofactura electrónica"/> -->
				<xs:enumeration value="Nota de crédito electrónica"/>
				<xs:enumeration value="Nota de débito electrónica"/>
<!-- 			<xs:enumeration value="Nota de remisión electrónica"/> -->
<!-- 			<xs:enumeration -->
<!-- 				value="Comprobante de retención electrónico"/> -->
		</xs:restriction>
	</xs:simpleType>

	
	<!-- Numero de timbrado -->
	<xs:simpleType name="tdNumTim">
		<xs:annotation>
			<xs:documentation>
				Numero de timbrado del documento electronico.
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="8"/>
					<xs:maxLength value="8"/>
					<xs:pattern value="0+[1-9][0-9]*|[1-9]+[0-9]+"/>
				</xs:restriction>			
			</xs:simpleType> 
		</xs:union>
	</xs:simpleType>

	<!-- Codigo de establecimiento proveido por el Sistema de timbrado -->
	<xs:simpleType name="tdEst">
		<xs:annotation>
			<xs:documentation>Codigo de establecimiento proveido por el Sistema
				de timbrado
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:pattern value="[0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo de Punto de Exp. proveido por el Sist.Timbrado -->
	<xs:simpleType name="tdPunExp">
		<xs:annotation>
			<xs:documentation>
				Codigo de Punto de Exp. proveido por el Sist.Timbrado
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="3"/>
			<xs:pattern value="[0-9]{3}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Numero de documento -->
	<xs:simpleType name="tdNumDoc">
		<xs:annotation>
			<xs:documentation>
				Numero de documento del DE
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="7"/>
			<xs:maxLength value="7"/>
			<xs:pattern value="0+[1-9][0-9]*|[1-9]+[0-9]+"/>
		</xs:restriction>
	</xs:simpleType>


	<!--Numero documento identidad -->
	<xs:simpleType name="tdNumDocId">
		<xs:annotation>
			<xs:documentation>
				Numero de documento de identidad
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Za-z\-]{1,20}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Fecha inicio de vigencia del Timbrado -->
	<xs:simpleType name="tdFeIniT">
		<xs:annotation>
			<xs:documentation>
				Fecha inicio de vigencia del Timbrado
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date">
			<xs:minInclusive value="2018-05-01"/>
			<xs:pattern
				value="[2-9][0-9]{3}-([0][1-9]|[1][0-2])-([0][0-9]|[1-2][0-9]|[3][0-1])"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Fecha desde implementacion vigencia de SIFEN -->
	<xs:simpleType name="tdFeIniS">
		<xs:annotation>
			<xs:documentation>
				Fecha inicio de vigencia de SIFEN
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:date">
			<xs:minInclusive value="2018-05-01"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Normalizador de Fecha y Hora -->
	<xs:simpleType name="fecHhmmss">
		<xs:annotation>
			<xs:documentation>
				Normalizador de Fecha y Hora AAAA-MM-DDThh:mm:ss
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:dateTime">
			<xs:pattern value="\d{4}-\d\d-\d\dT\d\d:\d\d:\d\d"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo impuesto al consumo afectado -->
	<xs:simpleType name="tiTImp">
		<xs:annotation>
			<xs:documentation>
				Tipo de impuesto al consumo afectado
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer"> <!-- Cambio temporal, solo se aceptará IVA -->
			<xs:enumeration value="1"/>
			<!-- <xs:enumeration value="2"/> -->
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del tipo de impuesto -->
	<xs:simpleType name="tdDesTImp">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de impuesto
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="IVA">
				<xs:annotation>
					<xs:documentation>
						Corresponde al codigo 1 del campo dDesTimp
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<!-- <xs:enumeration value="ISC">
				<xs:annotation>
					<xs:documentation>
						Corresponde al codigo 2 del campo dDesTimp
					</xs:documentation>
				</xs:annotation> 
			</xs:enumeration> -->
			<!-- <xs:enumeration value="ISC Importacion"> <xs:annotation> <xs:documentation> 
				Corresponde al codigo 3 del campo dDesTimp </xs:documentation> </xs:annotation> 
				</xs:enumeration> <xs:enumeration value="ISC Combustible"> <xs:annotation> 
				<xs:documentation> Corresponde al codigo 4 del campo dDesTimp </xs:documentation> 
				</xs:annotation> </xs:enumeration> <xs:enumeration value="ISC Cigarrillo"> 
				<xs:annotation> <xs:documentation> Corresponde al codigo 5 del campo dDesTimp 
				</xs:documentation> </xs:annotation> </xs:enumeration> <xs:enumeration value="Ninguno"> 
				<xs:annotation> <xs:documentation> Corresponde al codigo 9 del campo dDesTimp 
				</xs:documentation> </xs:annotation> </xs:enumeration> -->
		</xs:restriction>
	</xs:simpleType>

	<!-- RUC -->
	<xs:simpleType name="tRuc">
		<xs:restriction base="xs:string">
			<xs:minLength value="5"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="[1-9][0-9]*[0-9A-D]?"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Digito Verificador -->
	<xs:simpleType name="tDVer">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0-9]"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>


	<!-- Tipo de contribuyente -->
	<xs:simpleType name="tiTipCont">
		<xs:annotation>
			<xs:documentation>
				Tipo de contribuyente. 1(Persona Fisica), 2(Persona Juridica)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:whiteSpace value="collapse"/>			
			<xs:pattern value="[1-2]"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de Regimen -->
	<xs:simpleType name="tcTipReg">
		<xs:annotation>
			<xs:documentation>
				Tipo de Régimen
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value="[1-8]"/>
		</xs:restriction>

	</xs:simpleType>

	<!--Regimenes Aduaneros -->
	<!--TODO Enumerar los regimenes aduaneros -->
	<xs:simpleType name="tcTipRegAdu">
		<xs:annotation>
			<xs:documentation>
				Regimenes Aduaneros
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0-9]{4}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Ciudad de Emision del DE -->
	<xs:simpleType name="tcCiuEmi">
		<xs:restriction base="xs:integer">
			<xs:pattern value="[0-9]{1,5}"/>
			<xs:minInclusive value="1" />
			<xs:maxInclusive value="99999"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de la ciudad de emision -->
	<xs:simpleType name="tdDesCiuEmi">
		<xs:restriction base="noEmptyString">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Distrito -->
	<xs:simpleType name="tcDisEmi">
		<xs:annotation>
			<xs:documentation>
				Codigo de distrito
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:minInclusive value="1"/>
			<xs:pattern value="[0-9]{1,4}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion distrito -->
	<xs:simpleType name="tdDesDisEmi">
		<xs:annotation>
			<xs:documentation>
				Descripcion de distrito
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="noEmptyString">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Telefono del emisor - receptor-->
	<xs:simpleType name="tdTel">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{8,15}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Celular del emisor -->
	<xs:simpleType name="tdCel">
		<xs:restriction base="xs:string">
			<xs:pattern value="\d{10,20}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo de actividad economica -->
	<xs:simpleType name="tcActEco">
		<xs:annotation>
			<xs:documentation>
				Codigo de actividad economica del emisor segun la lista de maragantu
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="8"/>
			<xs:pattern value="[0-9A-Z]{1,8}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de actividad economica -->
	<xs:simpleType name="tdDesActEco">
		<xs:annotation>
			<xs:documentation>
				Descripcion de actividad economica
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="noEmptyString">
			<xs:minLength value="1"/>
			<xs:maxLength value="300"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Naturaleza del receptor -->
	<xs:simpleType name="tiNatRec">
		<xs:annotation>
			<xs:documentation>
				Naturaleza del receptor: 1(Contribuyente), 2(No Contribuyente)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[1-2]"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de Operacion -->
	<xs:simpleType name="tiTiOpe">
		<xs:annotation>
			<xs:documentation>
				Tipo de operacion: 1(B2B), 2(B2C), 3(B2G)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[1-3]"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo documento -->
	<xs:simpleType name="tiTipDoc">
		<xs:annotation>
			<xs:documentation>
				Tipo de documento de identidad del vendedor
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[1-4]"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo documento -->
	<xs:simpleType name="tiTipDocRec">
		<xs:annotation>
			<xs:documentation>
				Tipo de documento de identidad del receptor
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[1-5]"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de tipo de documento -->
	<xs:simpleType name="tdDtipDoc">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de documento de identidad del vendedor
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Cédula paraguaya"/>
			<xs:enumeration value="Pasaporte"/>
			<xs:enumeration value="Cédula extranjera"/>
			<xs:enumeration value="Carnet de residencia"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de tipo de documento -->
	<xs:simpleType name="tdDtipDocRec">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de documento de identidad del receptor
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Cédula paraguaya"/>
			<xs:enumeration value="Pasaporte"/>
			<xs:enumeration value="Cédula extranjera"/>
			<xs:enumeration value="Carnet de residencia"/>
			<xs:enumeration value="Innominado"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Condicion de operacion -->
	<xs:simpleType name="tiCondOpe">
		<xs:annotation>
			<xs:documentation>Condicion de la operacion</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:pattern value="[1-2]"/>
			<xs:enumeration value="1">
				<xs:annotation>
					<xs:documentation>Contado</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="2">
				<xs:annotation>
					<xs:documentation>Crédito</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de Condicion de Operacion -->
	<xs:simpleType name="tdDCondOpe">
		<xs:annotation>
			<xs:documentation>
				Descripcion de condicion de operacion
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Contado"/>
			<xs:enumeration value="Crédito"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de Pago -->
	<xs:simpleType name="tiTiPago">
		<xs:annotation>
			<xs:documentation>
				Codigo de tipo de pago
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="2"/>
			<xs:pattern value="[1-9]|1[0-6]"/>
			<xs:enumeration value="1"/>
			<!--Efectivo -->
			<xs:enumeration value="2"/>
			<!--Cheque -->
			<xs:enumeration value="3"/>
			<!--Tarjeta de Credito -->
			<xs:enumeration value="4"/>
			<!--Tarjeta de Debito -->
			<xs:enumeration value="5"/>
			<!--Transferencia -->
			<xs:enumeration value="6"/>
			<!--Giro -->
			<xs:enumeration value="7"/>
			<!--Billetera Electronica -->
			<xs:enumeration value="8"/>
			<!--Tarjeta Empresarial -->
			<xs:enumeration value="9"/>
			<!-- Vales -->
			<xs:enumeration value="10"/>
			<!-- Retención -->
			<xs:enumeration value="11"/>
			<!-- Anticipo -->
			<xs:enumeration value="12"/>
			<!-- Valores Fiscales -->
			<xs:enumeration value="13"/>
			<!-- Valores Comerciales -->
			<xs:enumeration value="14"/>
			<!-- Compensaciones -->
			<xs:enumeration value="15"/>
			<!-- Permuta -->
			<xs:enumeration value="16"/>
			<!-- Pago bancario -->
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del tipo de pago -->
	<xs:simpleType name="tdDesTiPag">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de pago
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Efectivo" />
			<xs:enumeration value="Cheque" />
			<xs:enumeration value="Tarjeta de crédito" />
			<xs:enumeration value="Tarjeta de débito" />
			<xs:enumeration value="Transferencia" />
			<xs:enumeration value="Giro" />
			<xs:enumeration value="Billetera electrónica" />
			<xs:enumeration value="Tarjeta empresarial" />
			<xs:enumeration value="Vale" />
			<xs:enumeration value="Retención" />
			<xs:enumeration value="Anticipo" />
			<xs:enumeration value="Valor fiscal" />
			<xs:enumeration value="Valor comercial" />
			<xs:enumeration value="Compensación" />
			<xs:enumeration value="Permuta" />
			<xs:enumeration value="Pago bancario" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo de tipo de tarjeta -->
	<xs:simpleType name="tiDenTarj">
		<xs:annotation>
			<xs:documentation>
				Codigo de tipo de tarjeta
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="1"/>
			<!-- VISA -->
			<xs:enumeration value="1"/>
			<!--MASTERCARD -->
			<xs:enumeration value="2"/>
			<!--AMERICAN EXPRESS -->
			<xs:enumeration value="3"/>
			<!-- MAESTRO -->
			<xs:enumeration value="4"/>
			<!--PANAL -->
			<xs:enumeration value="5"/>
			<!--CABAL -->
			<xs:enumeration value="6"/>
			<!-- OTRO -->
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de tipo de tarjeta -->
	<xs:simpleType name="tdDesDenTarj">
		<xs:annotation>
			<xs:documentation>
				Descripción de denominación de la tarjeta
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="4" />
					<xs:maxLength value="20" />
					<xs:enumeration value="Visa" />
					<xs:enumeration value="Mastercard" />
					<xs:enumeration value="American Express" />
					<xs:enumeration value="Maestro" />
					<xs:enumeration value="Panal" />
					<xs:enumeration value="Cabal" />
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<!--INFORMAR SI EN EL CAMPO iDenTarj se informo el codigo 9 -->
				<xs:restriction base="xs:string">
					<xs:minLength value="4"/>
					<xs:maxLength value="20" />
					<xs:pattern value=".+" />
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>

	<!-- Forma de procesamiento de pago -->
	<!--<xs:element name="iForProPa"> -->
	<xs:simpleType name="tiForProPa">
		<xs:annotation>
			<xs:documentation>
				Forma de procesamiento del pago. 1(POS), 2(Pago Electronico)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:short">
			<xs:totalDigits value="1"/>
			<xs:enumeration value="1"/>
			<!-- POS -->
			<xs:enumeration value="2"/>
			<!-- Pago Electronico (Ej. Compras por Internet) -->
			<xs:enumeration value="3"/>
			<!-- Otro -->
		</xs:restriction>
	</xs:simpleType>

	<!-- Numero de aprobacion de la operacion -->
	<!--<xs:element name="dNunapOpe"> -->
	<xs:simpleType name="tdNunApOpe">
		<xs:annotation>
			<xs:documentation>
				Código de autorización de la operación
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="10" />
		</xs:restriction>
	</xs:simpleType>

	<!--Numero de cheque -->
	<xs:simpleType name="tNumCheq">
		<xs:annotation>
			<xs:documentation>
				Numero de cheque
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="8" />
			<xs:maxLength value="8" />
			<xs:pattern value="[0-9]{8}" />
		</xs:restriction>
	</xs:simpleType>

	<!--Descripción de la condición de la operación a crédito -->
	<xs:simpleType name="tdDCondCred">
		<xs:annotation>
			<xs:documentation>
				Descripción de la condición de la operación a crédito
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Plazo" />
			<!-- Corresponde al Codigo 1 -->
			<xs:enumeration value="Cuota" />
			<!--Corresponde al Codigo 2 -->
		</xs:restriction>
	</xs:simpleType>

	<!-- Fecha: Formato AAAAMMDD -->
	<xs:simpleType name="tFecAAAAMMDD">
		<xs:restriction base="xs:string">
			<xs:pattern
				value="[2-9][0-9]{3}([0][1-9]|[1][0-2])([0][0-9]|[1-2][0-9]|[3][0-1])" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Fecha: Formato AAAA-MM-DD -->
	<xs:simpleType name="tFecAAAAMMDDguion">
		<xs:restriction base="xs:string">
			<xs:pattern
				value="[2-9][0-9]{3}-([0][1-9]|[1][0-2])-([0][0-9]|[1-2][0-9]|[3][0-1])" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Modalidad - Codigo de contratacion emitido por la DNCP -->
	<xs:simpleType name="tdModCont">
		<xs:annotation>
			<xs:documentation>
				Modalidad - Codigo emitido por la DNCP
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="noEmptyString">
			<xs:length value="2" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Entidad - Codigo de contratacion emitido por la DNCP -->
	<xs:simpleType name="tdEntCont">
		<xs:annotation>
			<xs:documentation>
				Entidad - Codigo de contratacion emitido por la DNCP
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="[0-9]{5}" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Anho - Codigo de contratacion emitido por la DNCP -->
	<xs:simpleType name="tdAnoCont">
		<xs:annotation>
			<xs:documentation>
				Anho - Codigo de contratacion emitido por la DNCP
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="2" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Secuencia - Codigo de contratacion emitido por la DNCP -->
	<xs:simpleType name="tdSecCont">
		<xs:annotation>
			<xs:documentation>
				Secuencia - Codigo de contratacion emitido por la DNCP
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="[0-9]{7}" />
			<xs:minInclusive value="1" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Condición del tipo de cambio -->
	<xs:simpleType name="tdCondTiCam">
		<xs:annotation>
			<xs:documentation>
				Condición del tipo de cambio 1(Global), 2(Por ítem)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:short">
			<xs:totalDigits value="1" />
			<xs:enumeration value="1" />
			<!-- GLOBAL -->
			<xs:enumeration value="2" />
			<!-- POR ITEM -->
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo Interno de Mercaderia -->
	<xs:simpleType name="tdCodInt">
		<xs:annotation>
			<xs:documentation>
				Codigo interno de mercaderia
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="20" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Partida arancelaria -->
	<xs:simpleType name="tdParAranc">
		<xs:annotation>
			<xs:documentation>
				Partida arancelaria
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="[0-9]{4}" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Nomenclatura comun del MERCOSUR -->
	<xs:simpleType name="tdNCM">
		<xs:annotation>
			<xs:documentation>
				Nomenclatura comun del MERCOSUR (NCM)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="[0-9]{6,8}" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo DNCP Nivel General -->
	<xs:simpleType name="tdDncpG">
		<xs:annotation>
			<xs:documentation>
				Codigo DNCP - Nivel General
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{8}" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo DNCP Nivel Especifico -->
	<xs:simpleType name="tdDncpE">
		<xs:annotation>
			<xs:documentation>
				Codigo DNCP - Nivel Especifico
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{3,4}" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Codigo GTIN (Producto) -->
	<xs:simpleType name="tdGtin">
		<xs:annotation>
			<xs:documentation>
				Codigo GTIN por Producto y por Paquete
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:pattern value="[0-9]{8,14}" />
		</xs:restriction>
	</xs:simpleType>
	
	<!--Cantidad de producto y/o servicio -->
	<xs:simpleType name="tdCantProSer">
		<xs:annotation>
			<xs:documentation>
				Cantidad del producto/servicio
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="14"/>
			<xs:fractionDigits value="4"/>
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="9999999999.9999"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Representacion de los monto (base) 15 dígitos enteros y 8 decimales -->
	<xs:simpleType name="tMontoBase">
		<xs:annotation>
			<xs:documentation>
				Tipo base para los atributos de monto
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="23"/>
			<xs:fractionDigits value="8"/>
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999999999999999.99999999"/>
		</xs:restriction>
	</xs:simpleType>
	
	
	<!--Representacion de los montos con 15 dígitos enteros y 4 decimales -->
	<xs:simpleType name="tMontoBase4">
		<xs:annotation>
			<xs:documentation>
				Tipo base para los atributos de monto
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:decimal">
			<xs:totalDigits value="19"/>
			<xs:fractionDigits value="4"/>
			<xs:minInclusive value="0"/>
			<xs:maxInclusive value="999999999999999.9999"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Forma de afectacion del IVA -->
	<xs:simpleType name="tiAfecIVA">
		<xs:annotation>
			<xs:documentation>
				Forma de afectacion del IVA 1(Gravado), 2(Exonerado), 3(Exento),
				4(Gravado parcial)
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="1" />
			<xs:enumeration value="2" />
			<xs:enumeration value="3" />
			<xs:enumeration value="4" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion de IVA -->
	<xs:simpleType name="tdDesAfecIVA">
		<xs:annotation>
			<xs:documentation>
				Descripcion de la afectacion del IVA
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Gravado IVA" />
			<!-- Corresponde al Codigo 1 del campo iAfecIVA -->
			<xs:enumeration
				value="Exonerado (Art. 83- Ley 125/91)" />
			<!--Corresponde al Codigo 2 del campo iAfecIVA -->
			<xs:enumeration value="Exento" />
			<!--Corresponde al Codigo 3 del campo iAfecIVA -->
			<xs:enumeration value="Gravado parcial (Grav- Exento)" />
			<!--Corresponde al Codigo 4 del campo iAfecIVA -->
		</xs:restriction>
	</xs:simpleType>

	<!--Proporcion del IVA -->
	<xs:simpleType name="tdPropIVA">
		<xs:annotation>
			<xs:documentation>
				Proporción gravada de IVA
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="3" />
			<xs:minInclusive value="0" />
			<xs:maxInclusive value="100" />
		</xs:restriction>
	</xs:simpleType>

	<!--Distincion de la tasa de IVA -->
	<xs:simpleType name="tdTasaIVA">
		<xs:annotation>
			<xs:documentation>
				Tasa del IVA
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:totalDigits value="2" />
			<xs:minInclusive value="0" />
		</xs:restriction>
	</xs:simpleType>

	<!--Categoria de ISC -->
	<xs:simpleType name="tcCatISC">
		<xs:annotation>
			<xs:documentation>
				Categoria de ISC
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1" />
			<xs:enumeration value="2" />
			<xs:enumeration value="3" />
			<xs:enumeration value="4" />
			<xs:enumeration value="5" />
		</xs:restriction>
	</xs:simpleType>

	<!--Descripcion de categoria de ISC -->
	<xs:simpleType name="tdDesCatISC">
		<xs:annotation>
			<xs:documentation>
				Descripcion de categoria de ISC
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration
				value="SECCION I-(Cigarrillos,Tabacos,Esencias y Otros derivados del Tabaco)" />
			<xs:enumeration
				value="SECCION II - (Bebidas con y sin alcohol)" />
			<xs:enumeration
				value="SECCION III - (Alcoholes y Derivados del alcohol)" />
			<xs:enumeration value="SECCION IV - (Combustibles)" />
			<xs:enumeration
				value="SECCION V - (Artículos considerados de lujo)" />
		</xs:restriction>
	</xs:simpleType>

	<!--Tasa del ISC -->
	<xs:simpleType name="tcTasaISC">
		<xs:annotation>
			<xs:documentation>
				Tasa del ISC
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<xs:enumeration value="1" />
			<!-- Corresponde al 1% -->
			<xs:enumeration value="2" />
			<!-- Corresponde al 5% -->
			<xs:enumeration value="3" />
			<!-- Corresponde al 9% -->
			<xs:enumeration value="4" />
			<!-- Corresponde al 10% -->
			<xs:enumeration value="5" />
			<!-- Corresponde al 11% -->
			<xs:enumeration value="6" />
			<!-- Corresponde al 13% -->
			<xs:enumeration value="7" />
			<!-- Corresponde al 16% -->
			<xs:enumeration value="8" />
			<!-- Corresponde al 18% -->
			<xs:enumeration value="9" />
			<!-- Corresponde al 20% -->
			<xs:enumeration value="10" />
			<!-- Corresponde al 24% -->
			<xs:enumeration value="11" />
			<!-- Corresponde al 34% -->
			<xs:enumeration value="12" />
			<!-- Corresponde al 38% -->
		</xs:restriction>
	</xs:simpleType>

	<!--Tipo de operacion de venta de vehiculos -->
	<xs:simpleType name="tiTipOpVN">
		<xs:annotation>
			<xs:documentation>
				Tipo de operacion de venta de vehiculos
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:short">
			<!-- Venta a representante -->
			<xs:enumeration value="1"/>
			<!-- Venta al Consumidor Final -->
			<xs:enumeration value="2"/>
			<!-- Venta al Gobierno -->
			<xs:enumeration value="3"/>
			<!-- Venta a Flota de Vehiculos -->
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Descripcion del tipo de venta de vehiculos -->
	<xs:simpleType name="tdDesTipOpVN">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de venta de vehiculos
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Venta a representante" />
					<!-- Codigo 1 del campo iTipOpVN -->
					<xs:enumeration value="Venta al Consumidor final"/>
					<!-- Codigo 2 del campo iTipOpVN -->
					<xs:enumeration value="Venta a gobierno"/>
					<!-- Codigo 3 del campo iTipOpVN -->
					<xs:enumeration value="Venta a flota de vehículos"/>
					<!-- Codigo 4 del campo iTipOpVN -->
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="10"/>
					<xs:maxLength value="50"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>

	<!--Chasis -->
	<xs:simpleType name="tdChasis">
		<xs:annotation>
			<xs:documentation>
				Chasis
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Za-z]{17}"/>
		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de combustible -->
	<xs:simpleType name="tiTipCom">
		<xs:annotation>
			<xs:documentation>
				Tipo de combustible
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<!-- Gasolina -->
			<xs:enumeration value="1"/>
			<!-- Diesel -->
			<xs:enumeration value="2"/>
			<!-- Etanol -->
			<xs:enumeration value="3"/>
			<!-- GNV -->
			<xs:enumeration value="4"/>
			<!--Flex -->
			<xs:enumeration value="5"/>
			<!--OTRO -->
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>

	<!--Descripcion del tipo de combustible -->
	<xs:simpleType name="tdDesTipCom">
		<xs:annotation>
			<xs:documentation>
				Descripcion del tipo de combustible
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Gasolina"/>
					<xs:enumeration value="Diésel"/>
					<xs:enumeration value="Etanol"/>
					<xs:enumeration value="GNV"/>
					<xs:enumeration value="Flex"/>
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="3"/>
					<xs:maxLength value="20"/>
					<xs:pattern value=".+"/>
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>

	<!--Tipo de pintura -->
	<!--<xs:element name="iTipoPint"> -->
	<xs:simpleType name="tiTipoPint">
		<xs:annotation>
			<xs:documentation>
				Tipo de pintura del vehiculo
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="1" />
			<!-- Solida -->
			<xs:enumeration value="2" />
			<!-- Metalica -->
			<xs:enumeration value="3" />
			<!-- Perla -->
			<xs:enumeration value="4" />
			<!-- Fosca -->
			<xs:enumeration value="9" />
			<!-- Otro -->
		</xs:restriction>
	</xs:simpleType>

	<!--Descripcion de tipo de pintura -->
	<!--<xs:element name="dDesTP"> -->
	<xs:simpleType name="tdDesTP">
		<xs:annotation>
			<xs:documentation>
				Description del tipo de pintura
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Solida"/>
					<!-- Corresponde al codigo 1 del campo iTipoPint -->
					<xs:enumeration value="Metalica" />
					<!-- Corresponde al codigo 2 del campo iTipoPint -->
					<xs:enumeration value="Perla"/>
					<!-- Corresponde al codigo 3 del campo iTipoPint -->
					<xs:enumeration value="Fosca"/>
					<!-- Corresponde al codigo 4 del campo iTipoPint -->
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:minLength value="4"/>
					<xs:maxLength value="10"/>
					<xs:pattern value="[A-Za-z0-9]" />
				</xs:restriction>
			</xs:simpleType>
		</xs:union>
	</xs:simpleType>

	<!--Tipo de vehiculo -->
	<!--TODO: Codificar de acuerdo al departamento de registro vehicular -->
	<!--<xs:element name="cTipVeh"> -->
	<xs:simpleType name="tcTipVeh">
		<xs:annotation>
			<xs:documentation>
				Tipo de vehículo
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="4"/>
			<xs:maxLength value="10"/>
		</xs:restriction>
	</xs:simpleType>

	

	<!--Condicion del vehiculo -->
	<xs:simpleType name="tiCondVeh">
		<xs:annotation>
			<xs:documentation>
				Codigo de condicion del vehiculo
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:short">
			<xs:enumeration value="1" />
			<xs:enumeration value="2" />
			<xs:enumeration value="3" />
		</xs:restriction>
	</xs:simpleType>

	<!--Descripcion del tipo de vehiculo -->
	<xs:simpleType name="tdDesCondVeh">
		<xs:restriction base="xs:string">
			<xs:enumeration value="Acabado" />
