		</xs:restriction>
	</xs:simpleType>

	<!-- Tipo de documento asociado -->
	<xs:simpleType name="tiTipDocAso">
		<xs:annotation>
			<xs:documentation>
				Tipo de documento asociado
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">
			<!-- 1= Electrónico -->
			<xs:enumeration value="1" />
			<!-- 2= Impreso -->
			<xs:enumeration value="2" />
