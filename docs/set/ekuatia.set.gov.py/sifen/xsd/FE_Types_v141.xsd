			<xs:enumeration value="Nota de débito" />
			<xs:enumeration value="Nota de remisión" />
			<xs:enumeration value="Comprobante de retención" />
		</xs:restriction>
	</xs:simpleType>

	<!--Motivo de emision de nota de credito/debito -->
	<xs:simpleType name="tiMotEmi">
		<xs:annotation>
			<xs:documentation>
