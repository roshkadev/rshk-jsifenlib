	                    </xs:documentation>
	                </xs:annotation>
	                <xs:simpleType>
	                	<xs:restriction base="xs:positiveInteger">
							<xs:totalDigits value="20" />
						</xs:restriction>
	                </xs:simpleType>
	        </xs:element>
	        <xs:element name="cUniMedTotPes" minOccurs="0" type="tcUniMed"/>
			<xs:element name="dDesUniMedTotPes" minOccurs="0" type="tdDesUniMed"/>
			<xs:element name="dTotPesMerc" minOccurs="0">
	                <xs:annotation>
	                    <xs:documentation>
	                        Total peso de la mercadería
	                    </xs:documentation>
	                </xs:annotation>
	                <xs:simpleType>
	                	<xs:restriction base="xs:positiveInteger">
							<xs:totalDigits value="20" />
						</xs:restriction>
	                </xs:simpleType>
	            </xs:element>
	            <xs:element name="iCarCarga" minOccurs="0">
	            	<xs:simpleType>
	                	<xs:restriction base="xs:positiveInteger">
							<xs:enumeration value="1"/>
							<xs:enumeration value="2"/>
							<xs:enumeration value="3"/>
						</xs:restriction>
	                </xs:simpleType>
	            </xs:element>
	            <xs:element name="dDesCarCarga" minOccurs="0">
		            <xs:simpleType>
						<xs:annotation>
							<xs:documentation>
								Descripción de las características de la carga
							</xs:documentation>
						</xs:annotation>
						<xs:union>
							<xs:simpleType>
								<xs:restriction base="xs:string">
									<xs:enumeration value="Mercaderías con cadena de frío"/>
									<xs:enumeration value="Carga peligrosa"/>
								</xs:restriction>
							</xs:simpleType>
							<xs:simpleType>
								<xs:restriction base="noEmptyString">
