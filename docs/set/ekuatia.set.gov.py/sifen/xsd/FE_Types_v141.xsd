					<xs:enumeration value="Participación en ferias" />
					<xs:enumeration value="Traslado de encomienda" />
					<xs:enumeration value="Decomiso" />
				</xs:restriction>
			</xs:simpleType>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:maxLength value="60" />
					<xs:minLength value="5" />
				</xs:restriction>
			</xs:simpleType>
