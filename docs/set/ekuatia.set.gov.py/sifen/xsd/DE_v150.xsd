            </xs:documentation>
        </xs:annotation>
        <xs:sequence>
	        <xs:element name="cUniMedTotVol" minOccurs="0" type="tcUniMed"/>
	        <xs:element name="dDesUniMedTotVol" minOccurs="0" type="tdDesUniMed"/>    
