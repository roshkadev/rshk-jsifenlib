			<xs:documentation>
				Responsable por el costo del flete
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:integer">

			<!-- 1= Emisor de la Factura Electrónica -->
			<xs:enumeration value="1" />
			<!-- 2= Receptor de la Factura Electrónica -->
			<xs:enumeration value="2" />
