	<!--Condicion de la negociacion -->
	<xs:simpleType name="tcCondNeg">
		<xs:annotation>
			<xs:documentation>
				Condicion de la negociacion
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="CFR">
				<xs:annotation>
					<xs:documentation>
						Costo y flete
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CIF">
				<xs:annotation>
					<xs:documentation>
						Costo, seguro y flete
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CIP">
				<xs:annotation>
					<xs:documentation>
						Transporte y seguros pagados hasta
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CPT">
				<xs:annotation>
					<xs:documentation>
						Transporte pagado hasta
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DAP">
				<xs:annotation>
					<xs:documentation>
						Entregada en el lugar convenido
					</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="DAT">
				<xs:annotation>
					<xs:documentation>
