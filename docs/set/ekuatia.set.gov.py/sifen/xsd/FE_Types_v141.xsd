			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:totalDigits value="5" />
		</xs:restriction>
	</xs:simpleType>

	<!--Descripcion de ciudades -->
	<xs:simpleType name="tDesCiudad">
		<xs:annotation>
			<xs:documentation>
				Ciudades
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="noEmptyString">
			<xs:maxLength value="30" />
		</xs:restriction>
	</xs:simpleType>

