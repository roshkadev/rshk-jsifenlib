	<xs:simpleType name="tdDMotivTras">
		<xs:annotation>
			<xs:documentation>
				Descripcion del motivo de emisión
			</xs:documentation>
		</xs:annotation>
		<xs:union>
			<xs:simpleType>
				<xs:restriction base="xs:string">
					<xs:enumeration value="Traslado por ventas" />
					<xs:enumeration value="Traslado por consignación" />
					<xs:enumeration value="Exportación" />
					<xs:enumeration value="Traslado por Compra" />
					<xs:enumeration value="Importación" />
					<xs:enumeration value="Traslado por devolución" />
