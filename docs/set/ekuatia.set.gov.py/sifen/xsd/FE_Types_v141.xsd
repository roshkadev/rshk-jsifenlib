			<xs:enumeration value="Impreso" />
			<xs:minLength value="1" />
		</xs:restriction>
	</xs:simpleType>

	<!--Tipo de documento impreso -->
	<xs:simpleType name="tiTIpoDoc">
		<xs:annotation>
			<xs:documentation>
				Tipos de documentos impresos
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="1" />
			<xs:enumeration value="2" />
			<xs:enumeration value="3" />
			<xs:enumeration value="4" />
			<xs:enumeration value="5" />
		</xs:restriction>
	</xs:simpleType>

	<!-- Descripcion del tipo de documento impreso -->
	<xs:simpleType name="tdDesTIpoDoc">
		<xs:annotation>
			<xs:documentation>
				Tipos de documentos impresos
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Factura" />
